            <string>///explode into child asteroids
bullet = other;

child1_direction = bullet.direction + 90 + random(45);
if(child1_direction &gt; 360) {
    child1_direction -= 360;
}

child2_direction = child1_direction + 180 - random(45);
if(child2_direction &gt; 360) {
    child2_direction -= 360;
}

offset_angle = random(360);
asteroid_width = sprite_get_width(spr_asteroid_s);
asteroid_height = sprite_get_height(spr_asteroid_s);

x_offset = asteroid_width * cos(offset_angle);
y_offset = asteroid_height * sin(offset_angle);

while( (x + (x_offset + asteroid_width)) &gt;= room_width) x-=1; 
while( (x - (x_offset + asteroid_width)) &lt;= 0) x+=1; 
while( (y + (y_offset + asteroid_height)) &gt;= room_height) y-=1; 
while( (y - (y_offset + asteroid_height)) &lt;= 0) y+=1; 

o = instance_create(x + x_offset, y + y_offset, obj_asteroid_s);
o = instance_create(x - x_offset, y - y_offset, obj_asteroid_s);



//make the particles
var o;
var i;
var num_particles = irandom(2) + 2;
var angle_offset = 360/num_particles;

for (i = 0; i &lt; num_particles; i+=1){
    o = instance_create(x, y, obj_dead_asteroid);
